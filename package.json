{
  "name": "docreator",
  "version": "0.0.0",
  "description": "my majestic project",
  "main": "dist/docreator.cjs.js",
  "bin": "bin/cli.js",
  "files": [
    "dist"
  ],
  "keywords": [],
  "scripts": {
    "build": "bili",
    "lint": "eslint src/** --fix",
    "test": "nyc ava",
    "commit": "git-cz",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -r 0",
    "release": "release-it --no-git.requireCleanWorkingDir",
    "prepublishOnly": "yarn build"
  },
  "repository": {
    "url": "zero1five/docreator",
    "type": "git"
  },
  "author": "zero1five<zerodengyin@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "antd": "^3.19.8",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.12.0",
    "babel-preset-react-app": "^9.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "happypack": "^5.0.1",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.7.0",
    "react": "^16.8.6",
    "react-dev-utils": "^9.0.1",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.11.1",
    "react-router-dom": "^5.0.1",
    "webpack": "^4.35.0",
    "webpack-dev-server": "^3.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.0.0",
    "@commitlint/read": "^7.1.2",
    "ava": "^1.0.1",
    "babel-eslint": "^10.0.2",
    "bili": "^3.4.2",
    "chalk": "^2.4.1",
    "commitizen": "^3.0.5",
    "conventional-changelog-cli": "^2.0.11",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "^5.11.1",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-node-core": "^1.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.14.2",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "nyc": "^13.1.0",
    "prettier": "^1.15.3",
    "release-it": "^9.4.3"
  },
  "lint-staged": {
    "*.{js}": [
      "npm run lint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "commit-msg": "node ./scripts/commit-lint.js"
    }
  },
  "ava": {
    "files": [
      "**/__test__/**/*.test.js",
      "**/__test__/**/*.spec.js"
    ],
    "require": [
      "@babel/register"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
