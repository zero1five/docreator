{
  "name": "docreator",
  "version": "0.0.0",
  "description": "my majestic project",
  "main": "dist/docreator.cjs.js",
  "files": [
    "dist"
  ],
  "keywords": [],
  "scripts": {
    "build": "bili",
    "lint": "eslint src/** --fix",
    "test": "nyc ava",
    "commit": "git-cz",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -r 0",
    "release": "release-it --no-git.requireCleanWorkingDir",
    "prepublishOnly": "yarn build"
  },
  "repository": {
    "url": "zero1five/docreator",
    "type": "git"
  },
  "author": "zero1five<zerodengyin@gmail.com>",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.0.0",
    "@commitlint/read": "^7.1.2",
    "ava": "^1.0.1",
    "bili": "^3.4.2",
    "chalk": "^2.4.1",
    "commitizen": "^3.0.5",
    "conventional-changelog-cli": "^2.0.11",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "^5.11.1",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "nyc": "^13.1.0",
    "prettier": "^1.15.3",
    "release-it": "^9.4.3"
  },
  "lint-staged": {
    "*.{js}": [
      "npm run lint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "commit-msg": "node ./scripts/commit-lint.js"
    }
  },
  "ava": {
    "files": [
      "**/__test__/**/*.test.js",
      "**/__test__/**/*.spec.js"
    ],
    "require": [
      "@babel/register"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}